{
  "name": "osrs-tracker-web",
  "version": "0.0.0",
  "private": true,
  "description": "Keep track of everything that matters in Old School Runescape. Track the latest news, item prices, hiscores, and XP gains.",
  "keywords": [
    "runescape",
    "osrs",
    "oldschool-runescape",
    "hiscores",
    "items",
    "price-tracker",
    "xp-tracker"
  ],
  "repository": {
    "type": "git",
    "url": "github:osrs-tracker/osrs-tracker-web"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Freek Mencke",
    "email": "freek_mencke.dev@outlook.com",
    "url": "https://github.com/osrs-tracker/osrs-tracker-web"
  },
  "scripts": {
    "build": "ng build",
    "predeploy": "ng build",
    "deploy": "npm run deploy:app && npm run deploy:assets",
    "deploy:app": "aws s3 sync --delete --exclude 'assets/*' --exclude '*.js' --exclude '*.css' ./dist/osrs-tracker-web s3://osrs-tracker.freekmencke.com",
    "deploy:assets": "aws s3 sync --delete --size-only ./dist/osrs-tracker-web s3://osrs-tracker.freekmencke.com",
    "lint": "ng lint",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "start": "ng serve --open",
    "test": "jest --watch",
    "test:ci": "jest",
    "watch": "ng build --watch --configuration development"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": "./.prettierrc.json",
  "dependencies": {
    "@angular/animations": "^15.2.5",
    "@angular/cdk": "^15.2.6",
    "@angular/common": "^15.2.5",
    "@angular/compiler": "^15.2.5",
    "@angular/core": "^15.2.5",
    "@angular/forms": "^15.2.5",
    "@angular/platform-browser": "^15.2.5",
    "@angular/platform-browser-dynamic": "^15.2.5",
    "@angular/router": "^15.2.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.5",
    "@osrs-tracker/models": "^0.3.3",
    "@tailwindcss/forms": "^0.5.3",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "jest-preset-angular": "^13.0.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "~4.9.4"
  }
}
